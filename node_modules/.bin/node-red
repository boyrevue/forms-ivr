#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node-red@4.1.0_typescript@5.9.2/node_modules/node-red/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node-red@4.1.0_typescript@5.9.2/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node-red@4.1.0_typescript@5.9.2/node_modules/node-red/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node-red@4.1.0_typescript@5.9.2/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-red/red.js" "$@"
else
  exec node  "$basedir/../node-red/red.js" "$@"
fi
