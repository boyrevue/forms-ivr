#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/fetch-sparql-endpoint@5.1.0/node_modules/fetch-sparql-endpoint/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/fetch-sparql-endpoint@5.1.0/node_modules/fetch-sparql-endpoint/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/fetch-sparql-endpoint@5.1.0/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/fetch-sparql-endpoint@5.1.0/node_modules/fetch-sparql-endpoint/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/fetch-sparql-endpoint@5.1.0/node_modules/fetch-sparql-endpoint/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/fetch-sparql-endpoint@5.1.0/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fetch-sparql-endpoint/bin/fetch-sparql-endpoint.js" "$@"
else
  exec node  "$basedir/../fetch-sparql-endpoint/bin/fetch-sparql-endpoint.js" "$@"
fi
