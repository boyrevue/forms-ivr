#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/sparqljs@3.7.3/node_modules/sparqljs/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/sparqljs@3.7.3/node_modules/sparqljs/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/sparqljs@3.7.3/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/sparqljs@3.7.3/node_modules/sparqljs/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/sparqljs@3.7.3/node_modules/sparqljs/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/sparqljs@3.7.3/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../sparqljs@3.7.3/node_modules/sparqljs/bin/sparql-to-json" "$@"
else
  exec node  "$basedir/../../../../../../sparqljs@3.7.3/node_modules/sparqljs/bin/sparql-to-json" "$@"
fi
