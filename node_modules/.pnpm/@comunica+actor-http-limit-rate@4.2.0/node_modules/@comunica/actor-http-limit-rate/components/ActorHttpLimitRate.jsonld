        {
          "@id": "cahlr:components/ActorHttpLimitRate.jsonld#ActorHttpLimitRate_args_correctionMultiplier",
          "range": "xsd:float",
          "default": "0.1",
          "comment": "Multiplier for how aggressively the delay should follow the latest response time, ideally in range ]0.0, 1.0]."
        },
        {
          "@id": "cahlr:components/ActorHttpLimitRate.jsonld#ActorHttpLimitRate_args_failureMultiplier",
          "range": "xsd:float",
          "default": "10.0",
          "comment": "The response time of a failed request is taken into account with this multiplier applied."
        },
        {
          "@id": "cahlr:components/ActorHttpLimitRate.jsonld#ActorHttpLimitRate_args_limitByDefault",
          "range": "xsd:boolean",
          "default": "false",
          "comment": "Whether rate limiting should be applied from the first request onwards, instead of waiting for a request to fail. Enabling this behaviour can help avoid spamming a server with large amounts of requests when execution begins."
        },
        {
          "@id": "cahlr:components/ActorHttpLimitRate.jsonld#ActorHttpLimitRate_args_allowOverlap",
          "range": "xsd:boolean",
          "default": "false",
          "comment": "Whether requests should be allowed to overlap, instead of being effectively queued one after another for a host. Enabling this behaviour may help with overall performance, but will make the rate limiter less accurate, and make it impossible for the limiter to smooth out large bursts of requests to a given host."
        },
        {
          "@id": "cahlr:components/ActorHttpLimitRate.jsonld#ActorHttpLimitRate_args_name",
