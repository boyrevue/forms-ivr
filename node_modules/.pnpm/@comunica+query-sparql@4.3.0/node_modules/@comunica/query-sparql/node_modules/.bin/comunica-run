#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules/@comunica/runner-cli/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules/@comunica/runner-cli/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules/@comunica/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules/@comunica/runner-cli/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules/@comunica/runner-cli/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules/@comunica/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+runner-cli@4.2.0/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@comunica+runner-cli@4.2.0/node_modules/@comunica/runner-cli/bin/run.js" "$@"
else
  exec node  "$basedir/../../../../../../@comunica+runner-cli@4.2.0/node_modules/@comunica/runner-cli/bin/run.js" "$@"
fi
