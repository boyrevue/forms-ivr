#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules/@comunica/query-sparql/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules/@comunica/query-sparql/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules/@comunica/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules/@comunica/query-sparql/bin/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules/@comunica/query-sparql/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules/@comunica/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/@comunica+query-sparql@4.3.0/node_modules:/Users/vincentpower/DEV/FORMS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/query-dynamic.js" "$@"
else
  exec node  "$basedir/../../bin/query-dynamic.js" "$@"
fi
